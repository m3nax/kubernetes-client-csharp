{
    "metadata": [
        {
            "src": [
                {
                    "files": [
                        "src/KubernetesClient/KubernetesClient.csproj"
                    ],
                    "src": "../",
                    "exclude": [
                        "tests/**/**.csproj",
                        "**/bin/**",
                        "**/obj/**"
                    ]
                }
            ],
            "dest": "api",
            "disableGitFeatures": false,
            "disableDefaultFilter": false
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "api/**.yml",
                    "index.md",
                    "toc.yml"
                ]
            }
        ],
        "dest": "_site",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
            "default"
        ],
        "postProcessors": [],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false
    }
}
